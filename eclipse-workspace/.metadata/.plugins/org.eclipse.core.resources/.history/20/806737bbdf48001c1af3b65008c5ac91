package provafinal;

public class Pessoa{
	public String nome;
	public double renda;
	public Endereco endereco;
	public Conta conta;
	public Pessoa() {
		
	}
	public Pessoa(String nome, double renda, Endereco endereco, Conta conta) {
		this.nome = nome;
		this.renda = renda;
		this.endereco = endereco;
		this.conta = conta
	}
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public double getRenda() {
		return renda;
	}
	public void setRenda(double renda) {
		this.renda = renda;
	}
	public Endereco getEndereco() {
		return endereco;
	}
	public void setEndereco(Endereco endereco) {
		this.endereco = endereco;
	}
	public Conta getConta() {
		return conta;
	}
	public void setConta(Conta conta) {
		this.conta = conta;
	}
}
